
@{
    ViewData["Title"] = "DummyCarousel";
}

<div>
    <div id="coolCarousel" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner min-vh-100 h-100" role="listbox">
            <div class="carousel-item active">
                <img class="d-block w-100 img-fluid" src="~/images/wood-background.jpg" alt="First slide">
            </div>
            <div class="carousel-item">
                <img class="d-block w-100 img-fluid" src="~/images/forest-top-view.jpg" alt="Second slide">
            </div>
            <div class="carousel-item">
                <img class="d-block w-100 img-fluid" src="~/images/forest-top-view.jpg" alt="Third slide">
            </div>
        </div>
        <a class="carousel-control-prev" href="#coolCarousel" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#coolCarousel" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>



